
include @abs_top_builddir@/../CTrilinos/Makefile.export.CTrilinos

_FORTRILINOS_INCLUDES = \
  -I@abs_top_srcdir@/src \
  -I@abs_top_builddir@/src \
  $(CTRILINOS_INCLUDES)

_FORTRILINOS_LIBS = @LDFLAGS@ -L@abs_top_builddir@/src -lfortrilinos $(CTRILINOS_LIBS) @LIBS@

@USING_GNUMAKE_TRUE@FORTRILINOS_INCLUDES  = $(shell @PERL_EXE@ @abs_top_srcdir@/config/strip_dup_incl_paths.pl $(_FORTRILINOS_INCLUDES))
@USING_GNUMAKE_TRUE@FORTRILINOS_LIBS      = $(shell @PERL_EXE@ @abs_top_srcdir@/config/strip_dup_libs.pl $(_FORTRILINOS_LIBS))
@USING_GNUMAKE_FALSE@FORTRILINOS_INCLUDES = $(_FORTRILINOS_INCLUDES)
@USING_GNUMAKE_FALSE@FORTRILINOS_LIBS     = $(_FORTRILINOS_LIBS)
